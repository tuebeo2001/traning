}

class Student {
	private String name;
	private String ssn;
	private static int ID = 1234;
	private String email;
	
	public Student(String name, String ssn) {
	this.name = name;
	this.ssn = ssn;
	getemail();
	ID++;
	}
	private void getemail() {
		System.out.println(name+ "@email.com");
	}
	
}