package oop;
class person{
	String name;
	String email;
	String phoneString;
	
	void walk() {
		System.out.println(name + " is walking ");
	}
	
	void have() {
		System.out.println("acc" + email);
	}
	
	void call() {
		System.out.println("this " + phoneString);
	}
}

public class demo {
	public static void main(String[] args) {
	
	 person person1 = new person();
	 person1.name = "tur";
	 person1.walk();
	}
	
	

}
