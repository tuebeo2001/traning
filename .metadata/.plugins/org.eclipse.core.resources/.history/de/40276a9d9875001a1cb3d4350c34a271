package oop;
class person{
	String name;
	String email;
	String phoneString;
	
	void walk() {
		System.out.println(name + " is walking ");
	}
	
	void have() {
		System.out.println("acc" + email);
	}
	
	void call() {
		System.out.println("this " + phoneString);
	}
}

public class demo {

	static void call(String name) {
		System.out.println(name + " is dancing");
	}
	
	public static void main(String[] args) {
	call("tue");
	 
	}
	
	

}
